<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane styleClass="background" stylesheets="@labFX.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ilpider.games.freccette.view.ControllerLayoutRigaNumero">
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="3.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="btnPreso" alignment="CENTER" minWidth="50.0" mnemonicParsing="false" onAction="#doBtnPreso" styleClass="button-numero" textAlignment="CENTER" HBox.hgrow="NEVER">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
            <CheckBox fx:id="chkA" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin></CheckBox>
            <CheckBox fx:id="chkB" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" HBox.hgrow="ALWAYS" />
            <CheckBox fx:id="chkC" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></CheckBox>
            <Button fx:id="btnTogli" mnemonicParsing="false" onAction="#doBtnTogli" styleClass="button-togli" HBox.hgrow="NEVER" />
         </children>
         <padding>
            <Insets left="2.0" right="2.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
